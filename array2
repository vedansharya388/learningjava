import java .util.Scanner;
public class array2
{
    public void main()
    {
        Scanner sc=new Scanner(System.in);
        int ar[]=new int[5];
        int i,l,s,sum=0;
        for(i=0;i<5;i++)
        {
            System.out.println("enter a number");
          ar[i]=sc.nextInt();
        }
l=ar[0];
s=ar[0];
for(i=0;i<5;i++)
{
if(l<ar[i])
 l=ar[i];
 if(s>ar[i])
 s=ar[i];
}
System.out.println("smallest number is " +s);
System.out.println("largest number is " +l);
}
}
